essSlot(_IMPLEMENTATION_SLOT).value;\n     }\n     function _setImplementation(address newImplementation) internal {\n         require(newImplementation.code.length > 0);\n         StorageSlot.getAddressSlot(_IMPLEMENTATION_SLOT).value = newImplementation;\n     }\n }\n ```"},"fullyImplemented":true,"linearizedBaseContracts":[52107],"name":"StorageSlot","nameLocation":"1253:11:62","scope":52108,"usedErrors":[],"usedEvents":[]}],"license":"MIT"},"id":62}